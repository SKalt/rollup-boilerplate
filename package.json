{
  "name": "rollup-boilerplate",
  "version": "0.0.1",
  "description": "Short and sweet RollupJS boilerplate with babel support, livereload and PostCSS capabilities.",
  "main": "build/js/main.js",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "dev": "rollup -c --watch",
    "reload": "livereload 'build/'",
    "watch": "npm-run-all --parallel reload dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jake-wies/rollup-boilerplate.git"
  },
  "author": "Jake Wiesler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jake-wies/rollup-boilerplate/issues"
  },
  "homepage": "https://github.com/jake-wies/rollup-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.34",
    "@babel/preset-env": "^7.0.0-beta.34",
    "babel-eslint": "^8.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "cssnano": "^3.10.0",
    "eslint": "^4.12.1",
    "livereload": "^0.6.3",
    "npm-run-all": "^4.1.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-nested": "^2.1.2",
    "postcss-simple-vars": "^4.1.0",
    "rollup": "^0.52.1",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.5.5",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1"
  },
  "dependencies": {}
}
